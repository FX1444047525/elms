<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--suppress ALL -->
<mapper namespace="com.example.elms.dao.UserDao">

    <select id="getUserList" resultType="com.example.elms.model.User">

        select * from tab_user

    </select>

    <select id="login" resultType="com.example.elms.model.User">

        select * from tab_user where loginAct=#{loginAct} and loginPwd=#{loginPwd}

    </select>

	
</mapper>
























